{
  "1_1": {
    "arab": "بِسْمِ اللَّهِ الرَّحْمَٰنِ الرَّحِيمِ",
    "latin": "Bismillāhir-Raḥmānir-Raḥīm",
    "id": "Dengan nama Allah Yang Maha Pengasih, Maha Penyayang."
  },
  "1_2": {
    "arab": "الْحَمْدُ لِلَّهِ رَبِّ الْعَالَمِينَ",
    "latin": "Al-ḥamdu lillāhi rabbi al-‘ālamīn",
    "id": "Segala puji bagi Allah, Tuhan seluruh alam."
  },
  "1_3": {
    "arab": "الرَّحْمَٰنِ الرَّحِيمِ",
    "latin": "Ar-Raḥmānir-Raḥīm",
    "id": "Yang Maha Pengasih, Maha Penyayang."
  },
  "1_4": {
    "arab": "مَالِكِ يَوْمِ الدِّينِ",
    "latin": "Māliki yawmid-dīn",
    "id": "Yang Menguasai Hari Pembalasan."
  },
  "1_5": {
    "arab": "إِيَّاكَ نَعْبُدُ وَإِيَّاكَ نَسْتَعِينُ",
    "latin": "Iyyāka na‘budu wa iyyāka nasta‘īn",
    "id": "Hanya kepada Engkaulah kami menyembah dan hanya kepada Engkaulah kami mohon pertolongan."
  },
  "1_6": {
    "arab": "اهْدِنَا الصِّرَاطَ الْمُسْتَقِيمَ",
    "latin": "Ihdinas-ṣirāṭal-mustaqīm",
    "id": "Tunjukilah kami jalan yang lurus."
  },
  "1_7": {
    "arab": "صِرَاطَ الَّذِينَ أَنْعَمْتَ عَلَيْهِمْ...",
    "latin": "Ṣirāṭal-laḏīna an‘amta ‘alayhim...",
    "id": "Yaitu jalan orang-orang yang telah Engkau beri nikmat..."
  }
}

